{
  "name": "tar-vern",
  "description": "Tape archiver library for Typescript",
  "keywords": [
    "typescript",
    "tar",
    "async",
    "stream"
  ],
  "author": "Kouji Matsui (@kekyo@mi.kekyo.net)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekyo/tar-vern.git"
  },
  "homepage": "https://github.com/kekyo/tar-vern#readme",
  "readme": "README_pack.md",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "images/tar-vern-120-c.png"
  ],
  "scripts": {
    "build": "vite build",
    "test": "npm run build && vitest run",
    "dump": "npm run build && screw-up dump",
    "pack": "npm run build && screw-up pack --pack-destination artifacts/"
  },
  "devDependencies": {
    "@types/node": ">=20.0.0",
    "dayjs": ">=1.11.0",
    "screw-up": ">=0.13.0",
    "typescript": ">=5.0.0",
    "vite": ">=5.0.0",
    "vite-plugin-dts": ">=3.0.0",
    "vitest": ">=1.0.0"
  }
}
